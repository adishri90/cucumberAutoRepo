pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
	    
	    environment {
       env.PATH = env.PATH + ";c:\\Windows\\System32"
   }
                echo 'Building..'
				
				bat "mvn clean"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				bat "mvn test"
            }
        }
		
		        stage('Compile') {
            steps {
                echo 'Compiling..'
				bat "mvn compile"
            }
        }
		
		
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
